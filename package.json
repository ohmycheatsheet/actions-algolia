{
  "name": "actions-algolia",
  "version": "1.0.1",
  "description": "TypeScript Action Template",
  "keywords": [
    "GitHub",
    "Actions",
    "TypeScript"
  ],
  "license": "MIT",
  "homepage": "https://github.com/ohmycheatsheet/actions-algolia#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ohmycheatsheet/actions-algolia"
  },
  "bugs": {
    "url": "https://github.com/ohmycheatsheet/actions-algolia/issues",
    "email": "jiangweixian1994@gmail.com"
  },
  "author": "JW <jiangweixian1994@gmail.com> (https://twitter.com/jiangweixian)",
  "main": "dist/index.js",
  "scripts": {
    "lint:fix": "eslint . --fix",
    "build": "pnpm run prepare",
    "prepare": "ncc build ./src/index.ts -o dist --source-map --license licenses.txt && husky install",
    "test": "jest --setupFiles dotenv/config",
    "all": "npm run lint && npm run prepare && npm run test"
  },
  "lint-staged": {
    "**/**/*.{js,ts,vue,json}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@actions/core": "^1.5.0",
    "@actions/github": "^5.0.0",
    "@octokit/graphql": "^4.8.0",
    "algoliasearch": "^4.10.5",
    "dayjs": "^1.10.7",
    "lodash.omit": "^4.5.0"
  },
  "devDependencies": {
    "@aiou/eslint-config": "^0.3.2",
    "@changesets/cli": "^2.17.0",
    "@jest/types": "^27.1.1",
    "@types/jest": "^27.0.1",
    "@types/lodash.omit": "^4.5.6",
    "@vercel/ncc": "^0.31.0",
    "cz-emoji": "^1.3.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "husky": "^7.0.2",
    "jest": "^27.1.1",
    "lint-staged": "^11.1.2",
    "ts-jest": "^27.0.5",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-emoji"
    }
  }
}
